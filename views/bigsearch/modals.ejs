    <div id="view-groups" class="modal-window" style="display: none;">
        <div class="modal-title">
            <p class="title">Просмотр всех групп</p>
        </div>

        <span class="modal-close">×</span>

        <div class="modal-content">
            <div class="groups-aligment">

            </div>
        </div>
    </div>

    <!-- Настройки показа результатов -->
    <div id="view-properties" class="modal-window" style="display: none;">
      <div class="modal-title">
        <p class="title">Настройки показа</p>
      </div>
      <span class="modal-close">×</span>
      <div class="modal-content">
        <div class="properties-aligment">
          <form class="property properties-null">
            <p class="property-title property-null-title">Показывать нулевые значения?</p>
            <p class="property-option property-null-option"><input type="radio" name="null" value="0" />Нет</p>
            <p class="property-option property-null-option"><input type="radio" checked="checked" name="null" value="1" />Да</p>

          </form>
          <form class="property properties-separators">
            <p class="property-title property-separators-title">Какие разделители использовать?</p>
            <p class="property-option property-separators-option">
              <input type="radio" name="separators" value="1" />Перенос сроки
            </p>
            <p class="property-option property-separators-option">
              <input type="radio" name="separators" value="0">Запятые
            </p>
          </form>

        </div>
      </div>
    </div>
    <!-- Настройки показа результатов -->

    <!-- Сохранить шаблон поиска -->
    <div id="save-template" class="modal-window" style="display: none;">
