    <% layout('../layouts/layout') %>
    <link rel="stylesheet" type="text/css" href="css/views/dbschemaslist.css">
  <div class="dbinfo">
    <p class="dbinfo__title">Схемы БД</p>
    <p class="dbinfo__dbname">Вы выбрали базу данных <%=data.database%></p>
    <div>
  </div>
      <form method="POST" action="structure">
      <input type="hidden" name="dbname" value="<%= data.database%>">


      <div class="dbschemas">
        <ul class="dbschemas__list list-float-none">
        <% data.list.forEach(function(schema) { %>
          <li class="dbschemas__list__item standart-button">
            <p>
              <input type="radio" name="schemaname" value="<%= schema.nspname %>">
              <span><%= schema.nspname %></span>
            </p>
          </li>
        <% }); %>
        </ul>
      </div>


      <div class="submit">
        <button class="submit__button click-button" type="submit">      ДАЛЕЕ
        </button>
      </div>
      </form>
    </div>
