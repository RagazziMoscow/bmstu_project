
<% layout('../layouts/layout') %>
 <div>
 <form method="POST" action="dbschemas">
  <table class="database-select-table">
    <tr class="database-select-table__row">
      <th>
        <p>База данных</p>
      </th>
      <th>
        <p>Владелец</p>
      </th>
    </tr>

    <% data.forEach(function(item) { %>
    <tr class="database-select-table__row">
      <td>
        <p><input type="radio" name="dbname" value="<%= item.Database %>"><%= item.Database %></p>
      </td>
      <td>
        <p><%= item.Owner %></p>
      </td>
    </tr>
   <% }); %>
  </table>
  <div class="submit">
    <button class="submit__button" type="submit">Далее</button>
  </div>
  </form>
 </div>
